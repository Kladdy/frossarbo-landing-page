extends ../layouts/default.pug

block config

    //- Global Page Configuration
    - var pageTitle = 'Frossarbo 17'

block content

    //- * * * * * * * *
    //- * * Navbar  * *
    //- * * * * * * * *

    include mixins/navbar.pug
    +navbar({
        navbarBg: 'bg-white',
        navbarStyle: 'navbar-light',
        navbarBrandColor: 'text-dark',
        navbarBtnColor: 'btn-primary',
        navbarContainer: 'container px-5',
    })

    //- * * * * * * * * * *
    //- * * Page Header * *
    //- * * * * * * * * * *

    include mixins/header.pug
    +header({
        pageHeaderBg: 'bg-img-cover overlay overlay-light overlay-80',
        pageHeaderBgImg: 'background-image: url("https://source.unsplash.com/R-LK3sqLiBw/1600x1200")',
        pageHeaderStyle: 'page-header-ui-light',
        svgBorderAngled: false,
        svgBorderRounded: true,
        svgBorderWaves: false,
        svgBorderFill: 'text-light'
    })

        .page-header-ui-content.py-5.position-relative
            .container.px-5
                .row.gx-5.justify-content-center
                    .col-xl-8.col-lg-10.text-center
                        h1.page-header-ui-title.i18nelement(data-i18n='frossarbo17.title')
                        p.page-header-ui-text.mb-5.i18nelement(data-i18n='frossarbo17.description')

    //- * * * * * * * * *
    //- * * Portfolio * *
    //- * * * * * * * * *

    include mixins/section.pug
    +section({
        sectionBg: 'bg-light',
        sectionPadding: 'py-10',
        svgBorderAngled: false,
        svgBorderRounded: false,
        svgBorderWaves: false,
        svgBorderFill: ''
    })

        .container.px-5
            .row.gx-5
                .col-md-4
                    a.card.card-portfolio.mb-5(href='#!')
                        img.card-img-top(src='https://source.unsplash.com/jWQj5Wjepuk/400x300', alt='...')
                        .card-body
                            .card-title Book
                    a.card.card-portfolio.mb-5(href='#!')
                        img.card-img-top(src='https://source.unsplash.com/Ecnx13MEPK0/400x500', alt='...')
                        .card-body
                            .card-title Flower Mug
                    a.card.card-portfolio.mb-5.mb-md-0(href='#!')
                        img.card-img-top(src='https://source.unsplash.com/eUCy1jb_B7I/400x400', alt='...')
                        .card-body
                            .card-title Cactus Display
                .col-md-4
                    a.card.card-portfolio.mb-5(href='#!')
                        img.card-img-top(src='https://source.unsplash.com/cuTcfqsES6o/400x500', alt='...')
                        .card-body
                            .card-title Reading
                    a.card.card-portfolio.mb-5(href='#!')
                        img.card-img-top(src='https://source.unsplash.com/jipR1oTCO7U/400x400', alt='...')
                        .card-body
                            .card-title White Book
                    a.card.card-portfolio.mb-5.mb-md-0(href='#!')
                        img.card-img-top(src='https://source.unsplash.com/n3sqjJzZiBM/400x300', alt='...')
                        .card-body
                            .card-title Photo Face
                .col-md-4
                    a.card.card-portfolio.mb-5(href='#!')
                        img.card-img-top(src='https://source.unsplash.com/Zr7MxECDTQ8/400x300', alt='...')
                        .card-body
                            .card-title Catalog
                    a.card.card-portfolio.mb-5(href='#!')
                        img.card-img-top(src='https://source.unsplash.com/QcyenJDqDzw/400x500', alt='...')
                        .card-body
                            .card-title Notebook
                    a.card.card-portfolio.mb-5.mb-md-0(href='#!')
                        img.card-img-top(src='https://source.unsplash.com/TIutDBFEtcY/400x400', alt='...')
                        .card-body
                            .card-title Book Stand

    //- * * * * * * * * * * * * * * * *
    //- * * Contact Button Section  * *
    //- * * * * * * * * * * * * * * * *

    include mixins/section.pug
    +section({
        sectionBg: 'bg-white',
        sectionPadding: 'py-10',
        svgBorderAngled: false,
        svgBorderRounded: true,
        svgBorderWaves: false,
        svgBorderFill: 'text-dark'
    })

        .container.px-5
            .row.gx-5.justify-content-center
                .col-xl-6.col-lg-8.col-md-10.text-center.py-5
                    h2 Ready to get Started?
                    p.lead.text-gray-500.mb-5 We'd love to hear from you about your branding and digital needs. We are currently accepting new clients.
                    a(href='#!').btn.btn-primary.fw-500 Contact us

block footer

    include mixins/footer.pug
    +footer({
        footerBg: 'bg-dark',
        footerStyle: 'footer-dark'
    })